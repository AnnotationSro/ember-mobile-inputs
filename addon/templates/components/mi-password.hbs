<HtmlInput
  @dataInput={{this.dataInput}}
  @class='native-input password-input'
  @value={{this.value}}
  @type='password'
  @disabled={{this.disabled}}
  @readonly={{this.readonly}}
  @placeholder={{this.placeholder}}
  @keyUp={{action 'onKeyUp'}}
  @autocomplete={{this.autocomplete}}
></HtmlInput>